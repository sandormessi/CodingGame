<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<LangVersion>latest</LangVersion>
		<UseWpf>true</UseWpf>
		<Nullable>enable</Nullable>
		<StartupObject>CodeFileMergeApplication.App</StartupObject>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0"/>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\CodeFileMergeApplication.Core\CodeFileMergeApplication.Core.csproj"/>
		<ProjectReference Include="..\WpfApplication.Core\WpfApplication.Core.csproj"/>
	</ItemGroup>

</Project>